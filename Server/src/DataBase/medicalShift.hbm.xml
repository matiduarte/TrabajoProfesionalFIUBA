<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="entities.MedicalShift" table="MedicalShift">  
    <id name="id">  
     <generator class="increment"></generator>  
    </id>  
    
    <property name="date" type="int"></property>
    <property name="doctorId" type="int"></property>
    <property name="patientName"></property>
    
            
  </class>  
            
 </hibernate-mapping>
